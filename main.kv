<MainScreen>:
    name: 'main'
    bruhmst: bruhmst
    bruhmst2: bruhmst2
    bruhmst3: bruhmst3
    aaa: aaa
    on_enter: root.test()
    FloatLayout:
        size_hint: None, None

    CustomImage:
        source: "stuff.png"
        allow_stretch: True
        keep_ratio: True

    DPEAButton:
        id: asd
        size: 200, 100
        center_x: root.width * 0.1
        center_y: root.height * 0.25

        background_color: 0,0,0,0
        color: 0.839, 0.505, 0.168, 1

        text: "Idle On"
        on_press:root.idleTrue()
    DPEAButton:
        id: asg
        size: 200, 100
        center_x: root.width * 0.366
        center_y: root.height * 0.25

        background_color: 0,0,0,0
        color: 0.839, 0.505, 0.168, 1

        text: "Color Picker"
        on_press:root.colorScreen()

    DPEAButton:
        id: LMF
        size: 200, 100
        center_x: root.width * .9
        center_y: root.height * .25

        background_color: 0,0,0,0
        color: 0.035, 0.152, 0.725, 1

        text: "Center"
        on_press:root.whatThis()
    DPEAButton:
        id: WHITE
        size: 200, 100
        center_x: root.width * .6333
        center_y: root.height * .25

        background_color: 0,0,0,0
        color: 0.125, 0.619, 0.164, 1

        text: "To White"
        on_press:root.setWhite()
    Label:
        id: aaa
        font_size: 30
        x: root.width * 0
        y: root.height * -.3
        color: 1, 1, 1, 1
    Label:
        id: bruhmst
        text: "Red Light:"
        font_size: 30
        x: root.width * -.2
        y: root.height * .3
        color: 1, 1, 1, 1
    Label:
        id: bruhmst2
        text: "Blue Light:"
        font_size: 30
        x: root.width * 0
        y: root.height * .3
        color: 1, 1, 1, 1
    Label:
        id: bruhmst3
        text: "Green Light:"
        font_size: 30
        x: root.width * .2
        y: root.height * .3
        color: 1, 1, 1, 1
