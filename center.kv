<CenterScreen>:
    name: 'center'
    on_enter: root.oii()

    FloatLayout:
        size_hint: None, None

    CustomImage:
        source: "stuff.png"
        allow_stretch: True
        keep_ratio: True
    Label:
        text: "Centering..."
        font_size: 80
        x: root.width * 0
        y: root.height * 0
        color: 1, 1, 1, 1

